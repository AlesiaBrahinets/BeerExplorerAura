<aura:component >
    <aura:attribute name="accountRecords" type="Account[]"/>
    <b>Account Records</b>
    <c:ComponentMethod aura:id="child"/>
    <aura:iteration items="{!v.accountRecords}" var="item">
    <div>
        Name of Account - {!item.Name}
        </div>
    </aura:iteration>
    <lightning:button label="Call aura:method" onclick="{!c.callAuraMethodServerTrip}" />
</aura:component>